<div class="list-wrap">
<% @list.each do |list| %>
  <div class="list-box">
    <%= attchment_image_tag list, :image, :fill, 200, 200, format: 'jpeg' %>
    <div class="list-box">
      <span class="list-body"><%= list.body %></span>
      <span class="list-title"><%=link_to list.title, ttodolist_path(list.id) %></span>
    </div>
  </div>
<% end %>
</div>